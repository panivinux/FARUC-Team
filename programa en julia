using SpecialFunctions; println((function(n_max=10) Φ = (1 + √5) / 2; Λ_KK = Φ^5 * 1e16; V_C = (2π)^5 / 120; term_log = log(Λ_KK / Φ^2.5); ζ_zeros = [14.134725141, 21.022039639, 25.010857580, 30.424876126, 32.935061588, 37.586178159, 40.918719012, 43.327073281, 48.005150881, 49.773832478]; s = 0.0; for n in 1:n_max γ_n = ζ_zeros[n]; D_n = 4 + (-1)^n / Φ^n; term = (-1)^n * gamma(1 + n / Φ) / (abs(zeta(0.5 + im * γ_n)) * gamma(D_n + 1)); s += term; end; factor_normalizacion = 137.035999084 / ((3 * V_C / π^2) * term_log * s); return factor_normalizacion * (3 * V_C / π^2) * term_log * s end)())
